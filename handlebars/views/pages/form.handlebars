<div class="card bg-light" style="width: 500px;">
    <div class="card-body">
        <div class="mb-3 text-center">
            <h1 class="fw-bold text-success">Productos</h1>
        </div>
        <div class="mb-3">
            <label for="tituloInput1" class="form-label fw-bold fs-5">Titulo</label>
            <input type="text" class="form-control" id="tituloInput1" placeholder="titulo">
        </div>
        <div class="mb-3">
            <label for="precioInput1" class="form-label fw-bold fs-5">Precio</label>
            <input type="number" class="form-control" id="precioInput1" placeholder="precio">
        </div>
        <div class="mb-3">
            <label for="urlInput1" class="form-label fw-bold fs-5">Url</label>
            <input type="text" class="form-control" id="urlInput1" placeholder="url">
        </div>
        <div class="mt-3 text-center">
            <button type="button" class="btn btn-primary mb-3 fw-bold" onclick="enviarProducto()">Agregar Producto</button>
        </div>
    </div>
</div>
